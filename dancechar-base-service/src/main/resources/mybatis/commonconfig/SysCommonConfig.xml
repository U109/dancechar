<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.litian.dancechar.base.biz.commonconfig.dao.inf.SysCommonConfigDao">

    <select id="findList" parameterType="com.litian.dancechar.base.biz.commonconfig.dto.SysCommonConfigReqDTO"
            resultType="com.litian.dancechar.base.biz.commonconfig.dto.SysCommonConfigRespDTO">
        SELECT
        c.*
        FROM
        sys_common_config c
        WHERE
        c.delete_flag = 0
        <if test="configKey != null and configKey != ''">
            and c.config_key = #{configKey}
        </if>
    </select>
</mapper>
