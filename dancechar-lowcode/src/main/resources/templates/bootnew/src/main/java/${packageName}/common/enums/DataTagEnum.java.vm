package ${packageName}.common.enums;

/**
 * 活动审核状态ENUM
 *
 * @author ${authorName}
 * @date ${createDateString}
 */
public enum DataTagEnum {

    OPENID("openId", "微信ID"),
    USERID("userId", "会员ID"),
    MOBILE("mobile", "手机号")
    ;

    private String code;
    private String name;

    DataTagEnum(String code, String name) {
        this.code = code;
        this.name = name;
    }

    public String getCode() {
        return this.code;
    }

    public String getName() {
        return this.name;
    }

    public static String getNameByCode(String code) {
        for (DataTagEnum e : DataTagEnum.values()) {
            if (e.getCode().equalsIgnoreCase(code)) {
                return e.getName();
            }
        }
        return null;
    }
}
